{
  "expo": {
    "name": "ignite-gym",
    "slug": "ignite-gym",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",      
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
